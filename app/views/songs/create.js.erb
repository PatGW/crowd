$('#new-playlist-from-search').append('<%= j render "index" %>').submit(function(e) {
  e.preventDefault();
  var selected_song = $(this);
  var song_id = $(this).find('.song_id').val();
  console.log(song_id);

  if ( confirm("Are you sure you want to remove this Song?") )
    $.ajax({
      url: "/songs/" + song_id,
      type: "post",
      dataType: "json",
      data: {"_method":"delete"}
    }).success(function(data) {
      //$('#new-playlist-from-search').append($('form'));
    });
  return false;
});